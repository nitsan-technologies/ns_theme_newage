<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
               
                    <sheetTitle>Fields</sheetTitle>
               
                <type>array</type>
                <el>
                    <beforeText>
                       
                            <label>LLL:EXT:ns_theme_newage/Resources/Private/Language/locallang_flex.xlf:general.beforeText</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>trim,required</eval>
                            </config>
                       
                    </beforeText>
                    <iconInText>
                       
                           <label>LLL:EXT:ns_theme_newage/Resources/Private/Language/locallang_flex.xlf:general.icon</label>
                            <config>
                                <type>select</type>
                                <itemsProcFunc>NITSAN\NsThemeNewage\Backend\ItemsProcFunc->getFaIconsList</itemsProcFunc>
                                <renderType>selectSingle</renderType>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                            </config>
                       
                    </iconInText>
                    <afterText>
                       
                           <label>LLL:EXT:ns_theme_newage/Resources/Private/Language/locallang_flex.xlf:general.afterText</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>trim,required</eval>
                            </config>
                       
                    </afterText>
                    <contact>
                        <section>1</section>
                        <title>LLL:EXT:ns_theme_newage/Resources/Private/Language/locallang_flex.xlf:general.SocialLinks</title>
                        <type>array</type>
                        <el>
                            <container>
                                <title>LLL:EXT:ns_theme_newage/Resources/Private/Language/locallang_flex.xlf:general.item</title>
                                <type>array</type>
                                <el>
                                    <socialIcon>
                                       
                                            <label>LLL:EXT:ns_theme_newage/Resources/Private/Language/locallang_flex:general.icon</label>
                                            <config>
                                                <type>select</type>
                                                <itemsProcFunc>NITSAN\NsThemeNewage\Backend\ItemsProcFunc->getFabIconsList</itemsProcFunc>
                                                <renderType>selectSingle</renderType>
                                                <minitems>0</minitems>
                                                <maxitems>1</maxitems>
                                            </config>
                                       
                                    </socialIcon>
                                    <socialLink>
                                       
                                            <label>LLL:EXT:ns_theme_newage/Resources/Private/Language/locallang_flex.xlf:general.link
                                            </label>
                                            <exclude>0</exclude>
                                            <config>
                                                <type>input</type>
                                                <renderType>inputLink</renderType>
                                                <!--
                                                <fieldControl>
                                                    <linkPopup>
                                                        <options>
                                                            <blindLinkOptions>file,folder,mail,page,spec</blindLinkOptions>
                                                        </options>
                                                    </linkPopup>
                                                </fieldControl>
                                                -->
                                            </config>
                                       
                                    </socialLink>
                                    <SocialBgColor>
                                       
                                            <exclude>0</exclude>
                                            <label>LLL:EXT:ns_theme_newage/Resources/Private/Language/locallang_flex.xlf:general.SocialBgColor</label>
                                            <config>
                                                <type>input</type>
                                                <renderType>colorpicker</renderType>
                                                <size>10</size>
                                            </config>
                                       
                                    </SocialBgColor>
                                </el>
                            </container>
                        </el>
                    </contact>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>